{
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "enabled": {
      "type": "boolean"
    },
    "dns_settings": {
      "type": "object",
      "properties": {
        "target_domain": {
          "type": "string"
        },
        "dns_server": {
          "type": "string"
        },
        "timeout": {
          "type": "number"
        }
      },
      "required": [
        "target_domain"
      ]
    },
    "website_settings": {
      "type": "object",
      "properties": {
        "target_url": {
          "type": "string"
        },
        "timeout": {
          "type": "number"
        }
      },
      "required": [
        "target_url"
      ]
    },
    "response_controls": {
      "type": "object",
      "properties": {
        "delay_ms": {
          "type": "number"
        },
        "error_rate": {
          "type": "number"
        }
      }
    },
    "settings": {
      "type": "object",
      "properties": {
        "timeout": {
          "type": "number"
        },
        "retries": {
          "type": "integer"
        }
      },
      "required": [
        "timeout"
      ]
